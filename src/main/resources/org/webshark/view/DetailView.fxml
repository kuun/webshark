<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.webshark.view.DetailView">
   <children>
      <TreeTableView fx:id="headerInfoTree"
                     AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TreeTableColumn fx:id="colHeaderName" prefWidth="150" text="Header Name" />
          <TreeTableColumn fx:id="colHeaderValue" prefWidth="200" text="Header Value" />
        </columns>
      </TreeTableView>
   </children>
</AnchorPane>
