{
  "name": "webshark",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start-win": "set BROWSER=none && node ./node_modules/react-scripts/bin/react-scripts start",
    "build": "node ./node_modules/react-scripts/bin/react-scripts build",
    "test": "node ./node_modules/react-scripts/bin/react-scripts test",
    "eject": "node ./node_modules/react-scripts/bin/react-scripts eject",
    "electron-dev-win": "set ELECTRON_START_URL=http://localhost:5000 && node ./node_modules/electron/cli .",
    "dev": "node ./node_modules/foreman/nf start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.3",
    "foreman": "^3.0.1"
  }
}
